#Two Pointers

#Linked Lists
#Node has data and a pointer 
#Linked lists have multiple nodes

class Node:
    def __init__(self,value,nextNode=None):
        self.value=value
        self.nextNode=nextNode
        
        
Node1=Node(3)
Node2=Node(5)

Node1.nextNode=Node2






