nums=[1,4,5,6,3]

ya=[n for n in nums]
print(ya)

# I want n^2 for each n
yer= [n*n for n in nums]
print(yer)

#n for each n in nums if n is even, How to create a dictionary
#I want a letter and number pair for each letter in 'abcd' and '1234'

letters=['a','b','c','d']
numbers=[1,2,3,4]
print([(k,v) for k,v in zip(letters,numbers)])


string = ['hello']
print(string)
print([string[i:j] for i in range(len(string)) for j in range(i + 1, len(string) + 1)])


